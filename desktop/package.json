{
  "name": "primo",
  "version": "0.4.2",
  "main": "index.cjs",
  "description": "primo",
  "keywords": [],
  "author": "mateomorris",
  "license": "AGPL",
  "build": {
    "appId": "com.electron.primo",
    "productName": "Primo",
    "publish": "github",
    "afterSign": "notarize.cjs",
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "icon": "assets/icon.ico"
    },
    "mac": {
      "gatekeeperAssess": false,
      "hardenedRuntime": true,
      "entitlements": "entitlements.plist",
      "category": "public.app-category.developer-tools",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "dmg": {
      "sign": false,
      "icon": "assets/icon.icns"
    },
    "directories": {
      "buildResources": "assets"
    },
    "asar": true,
    "files": [
      "package.json",
      "index.cjs",
      "src/compile.cjs",
      "src/index.cjs",
      "src/preload.cjs",
      "build/**/*"
    ]
  },
  "scripts": {
    "dist": "npm run build && electron-builder --publish always",
    "dist:local": "npm run build && electron-builder",
    "electron": "electron .",
    "start": "concurrently \"npm:dev\" \"electron .\"",
    "dev": "svelte-kit dev -p 3333",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "dependencies": {
    "@primo-app/primo": "^1.5.10",
    "autoprefixer": "^10.4.0",
    "browser-image-compression": "^1.0.16",
    "check-internet-connected": "^2.0.5",
    "electron-is-dev": "^2.0.0",
    "electron-reload": "^1.5.0",
    "electron-serve": "^1.1.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.0.1",
    "electron-updater": "^4.6.5",
    "esm": "^3.2.25",
    "file-saver": "^2.0.5",
    "fs-extra": "^10.0.0",
    "idb-keyval": "^6.0.2",
    "install": "^0.13.0",
    "js-beautify": "^1.14.0",
    "jszip": "^3.7.1",
    "lodash-es": "^4.17.21",
    "mini-svg-data-uri": "^1.4.3",
    "node-fetch": "^2.6.7",
    "node-machine-id": "^1.1.12",
    "postcss": "^8.4.5",
    "postcss-nested": "^5.0.6",
    "prettier": "^2.4.1",
    "promise-worker": "^2.0.1",
    "rollup": "^2.73.0",
    "timeago.js": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-deb": "^6.0.0-beta.61",
    "@electron-forge/maker-dmg": "^6.0.0-beta.61",
    "@electron-forge/maker-rpm": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "@electron-forge/maker-zip": "^6.0.0-beta.61",
    "@iconify/svelte": "^2.2.1",
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@sveltejs/adapter-static": "^1.0.0-next.20",
    "@sveltejs/kit": "^1.0.0-next.326",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.11",
    "@tsconfig/svelte": "^2.0.1",
    "concurrently": "^6.4.0",
    "dotenv": "^14.2.0",
    "electron": "^15.5.5",
    "electron-builder": "^22.14.5",
    "electron-notarize": "^1.1.1",
    "electron-packager": "^15.4.0",
    "postcss-load-config": "^3.1.1",
    "postcss-preset-env": "^7.1.0",
    "svelte": "^3.47.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.10.2",
    "tailwindcss": "^2.2.19",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0",
    "vite": "^2.5.4"
  },
  "type": "module",
  "repository": "https://github.com/primo-af/primo.git",
  "private": false
}
